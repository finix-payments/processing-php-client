{
  "name": "finix/processing-php",
  "version": "1.0.4",
  "description": "A PHP HTTP Client conforming to the HAL hypermedia type for the Finix processing API",
  "license": "Apache2",
  "authors": [
    {
      "name": "Finix Development",
      "email": "dev@finixpayments.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle": "^5.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-0": {
      "Finix\\": "./src"
    },
    "psr-4": {
      "Finix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Finix\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
